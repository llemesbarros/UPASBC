{
  "manifest_version": 3,
  "name": "Link para receituário",
  "version": "1.0",
  "description": "Adiciona um botão na tela de atendimento de paciente do Hygia para o Receituário",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  }
}
